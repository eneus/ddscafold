    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/sites/default/settings.php": {
                  "mode": "replace",
                  "path": "./config/env/settings.php",
                  "overwrite": false
                },
                "[web-root]/sites/default/services.yml": {
                  "mode": "replace",
                  "path": "./config/env/services.yml",
                  "overwrite": false
                }
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "composer-exit-on-patch-failure": true,
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal 9 codebase  </>",
                "<bg=blue;fg=white>  for TSA Projects Manager project!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: <bg=yellow;fg=blue>drush si</>",
                "  * Read the user guide: <bg=yellow;fg=blue>README.md</>"
            ]
        },
        "patches": {
          "drupal/entity_browser": {
            "Drupal 9 - Error when configuring an entity browser views display": "https://www.drupal.org/files/issues/2020-06-23/entity_browser_views_display_error_3154079_2_D9.patch"
          }
        }
    }
